{
    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "lib": [
            "es2020"
        ],
        "declaration": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": false,
        "inlineSourceMap": true,
        "inlineSources": true,
        "experimentalDecorators": true,
        "strictPropertyInitialization": false,
        "typeRoots": [
            "./node_modules/@types",
            "node_modules/@types"
        ],
        "outDir": "/workspaces/openlawnz-api",
        "rootDir": ".",
        "sourceRoot": "/workspaces/openlawnz-api",
        "types": [
            "node"
        ]
    },
    "exclude": [
        "node_modules",
        "cdk.out"
    ]
}